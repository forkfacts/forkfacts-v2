{
  "name": "forkfacts",
  "version": "1.0.0",
  "private": true,
  "description": "forkfacts",
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "develop": "gatsby develop",
    "start": "npm run download-data && gatsby develop",
    "build": "npm run download-data && NODE_OPTIONS=--max-old-space-size=8192 gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "svgr": "npx @svgr/cli --out-dir src/designIcons --typescript --ignore-existing --replace-attr-values '#000=currentColor' src/images/icons",
    "download-data": "ts-node --experimentalSpecifierResolution node scripts/init.mjs",
    "build:env:dev": "cp .env.sample .env",
    "build:env:prod": "cp .env .env.production"
  },
  "dependencies": {
    "@algolia/autocomplete-core": "1.7.3",
    "@algolia/autocomplete-preset-algolia": "1.7.3",
    "@algolia/autocomplete-theme-classic": "1.7.3",
    "@algolia/client-search": "4.9.1",
    "@capacitor/core": "^5.0.5",
    "@capacitor/share": "^5.0.4",
    "@emotion/react": "^11.10.6",
    "@emotion/server": "latest",
    "@emotion/styled": "^11.10.6",
    "@fontsource/material-icons": "^4.5.4",
    "@fontsource/poppins": "^4.5.10",
    "@mdx-js/react": "^2.1.5",
    "@mui/icons-material": "^5.10.15",
    "@mui/material": "^5.11.14",
    "@mui/styles": "^5.10.15",
    "algoliasearch": "4.9.1",
    "classnames": "^2.3.2",
    "dotenv": "^16.0.3",
    "gatsby": "^5.10.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-config": "^1.0.7",
    "gatsby-plugin-emotion": "^8.8.0",
    "gatsby-plugin-image": "^3.2.0",
    "gatsby-plugin-manifest": "^5.0.0",
    "gatsby-plugin-material-ui": "^4.0.2",
    "gatsby-plugin-mdx": "^5.0.0",
    "gatsby-plugin-offline": "^6.11.0",
    "gatsby-plugin-seo": "^0.0.2",
    "gatsby-plugin-sharp": "^5.1.0",
    "gatsby-plugin-sitemap": "^6.0.0",
    "gatsby-plugin-sw": "^1.0.1",
    "gatsby-source-filesystem": "^5.7.0",
    "gatsby-theme-material-ui": "^5.3.0",
    "gatsby-transformer-json": "^5.7.0",
    "gatsby-transformer-sharp": "^5.1.0",
    "idb": "^7.1.1",
    "install": "^0.13.0",
    "konsta": "^2.0.0",
    "react": "^18.2.0",
    "react-circular-progressbar": "^2.1.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.3.0",
    "react-icons": "^4.8.0",
    "react-magic-slider-dots": "^1.2.2",
    "react-multi-carousel": "^2.8.2",
    "react-responsive": "^9.0.2",
    "react-share": "^4.4.1",
    "react-slick": "^0.29.0",
    "recharts": "^2.4.1",
    "slick-carousel": "^1.8.1",
    "ts-node": "^10.9.1",
    "zustand": "^4.3.6",
    "zx": "^7.2.1"
  },
  "devDependencies": {
    "@svgr/cli": "^6.5.1",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^18.0.8",
    "@types/react-slick": "^0.23.10",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^8.3.0",
    "gatsby-plugin-postcss": "^6.10.0",
    "husky": "^8.0.0",
    "postcss": "^8.4.23",
    "prettier": "2.8.0",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.8.4"
  }
}
